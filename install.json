{
  "run": [{
    "method": "shell.run",
    "params": { "message": "git clone https://github.com/radames/Real-Time-Latent-Consistency-Model app" }
  }, {
    "method": "shell.run",
    "params": {
      "path": "app/frontend",
      "message": ["npm install", "npm run build"]
    }
  }, {
    "method": "shell.run",
    "params": {
      "venv": "venv",
      "path": "app",
      "message": [
        "{{pip.install.torch}}",
        "{{gpu === 'nvidia' ? 'pip install xformers --index-url https://download.pytorch.org/whl/cu118' : ''}}",
        "pip install -r ../requirements.txt"
      ]
    }
  }, {
    "method": "input",
    "params": { "title": "Install Success", "description": "Go back to the dashboard and launch the app!" }
  }, {
    "method": "browser.open",
    "params": { "uri": "/?selected=Realtime LCM" }
  }]
}
